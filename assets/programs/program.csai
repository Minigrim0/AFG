fn turn_90() {
    let initial_rotation = $Rotation + 100;
    let current_rot = $Rotation - initial_rotation;
    while current_rot < 90 {
        current_rot = $Rotation - initial_rotation;
    }
    return;
}

fn main() {
    // Change velocity to 500
    $VelocityY = 500;

    if $Rays[3].type != 0 {
        if $Rays[3].dist < 1000 {
            $VelocityY = 0;
            $Moment = -10;
            if $Rotation > -90 {
                $VelocityY = 0;
            }
        }
    }
}
